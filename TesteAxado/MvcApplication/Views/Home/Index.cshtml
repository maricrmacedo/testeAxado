@model IEnumerable<MvcApplication.ViewModels.AvaliacoesViewModel>

@{
    ViewBag.Title = "Avaliações do Usuário";
}
@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title.</h1>
            </hgroup>
        </div>
    </section>
}
<ol class="round">
  <p>
   <b> @Html.ActionLink("Criar nova avaliação", "Create")</b>
</p>
<table class="table table-hover">
    <tr>
        <th>
            Id
        </th>
        <th>
            Transportadora
        </th>
        <th>
            Classificação
        </th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @item.Id
        </td>
        <td>
			@item.Transportadora.Descricao
        </td>
        <td>
			@item.Classificacao.Descricao
        </td>
    </tr>
}

</table>
  
</ol>
